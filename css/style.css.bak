	.header-top small{font-size:14px;}
	ul.hoby{margin-bottom:1em;}
	.about{margin:2em 0 3em;}
	.exp-devide h4{width:19%;}
	label.bag{left:24.7em;}
	.exp-devide-grid-right{left:28.3em;}
	.exp-devide-grid-left{left:2.8em;}
	.exp-devide-grid-rightb{
		top:18em;
		left:28.3em;
	}
	.education{margin-top:55%;}
	label.book{left:24.7em;}
	.skill-column{margin-bottom:2em;}
	.experience{min-height:1030px;}
	span.devide-line{height:435px;}
	.award-column{margin-bottom:1.5em;}
	.awards-top{padding:0;}
	.callbacks_tabs{left:41%;}
