	ul.social li i.ca{background-position:-225px 0px;}
	ul.social li i.ph{background-position:-283px 0px;}
	ul.social li i.gp{background-position:-339px 0px;}
	ul.social{margin:1em 0 0;}
	.social-media{padding:2em 0 1em;}
