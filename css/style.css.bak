	ul.hoby li i.shopping{background-position:-165px 0px;}
	ul.hoby li i.reading{background-position:-217px 0px;}
	ul.hoby li i.travel{background-position:-275px 0px;}
	ul.hoby li i.tea{background-position:-331px 0px;}
